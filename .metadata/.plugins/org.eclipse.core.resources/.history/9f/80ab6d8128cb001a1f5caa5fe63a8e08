
public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		for(int i=0;i<=4;i++)
		{
			System.out.println("Outerloopstarted");
			for(int j=1;j<=4-i;j++)
			{
				System.out.println("inner");
				System.out.println(j);
				j++;
			}
			System.out.println("Outercom");
		}

	}

}
