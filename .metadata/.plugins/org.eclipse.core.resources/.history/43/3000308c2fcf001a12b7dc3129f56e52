import java.util.ArrayList;

public class ArrayListExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList<String> a=new ArrayList<String>();
		a.add("Sadguna");
		a.add("Java");
		System.out.println(a);
		a.add(0, "Harry");
		System.out.println(a);
		a.remove(0);
		System.out.println(a);
		a.remove("Sadguna");
		
		System.out.println(a);

	}

}
